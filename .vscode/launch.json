  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Delete Data",
      "program": "${workspaceFolder}/seeder.js",
      "skipFiles": ["<node_internals>/**"],
      "args": ["-d"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Seed Data",
      "program": "${workspaceFolder}/seeder.js",
      "skipFiles": ["<node_internals>/**"],
      "args": ["-i"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\server.js",
      "console": "integratedTerminal"
    }
  ]
}
